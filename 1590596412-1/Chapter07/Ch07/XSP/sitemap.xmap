<?xml version="1.0"?>

<map:sitemap xmlns:map="http://apache.org/cocoon/sitemap/1.0">
  <map:components>
    <map:generators default="file">
      <map:generator name="xsp" src="org.apache.cocoon.generation.ServerPagesGenerator"/>
    </map:generators>
  </map:components>

  <map:pipelines>
    <map:pipeline>
      <map:match pattern="StockQuote.xsp">
        <map:generate type="xsp" src="StockQuote.xsp"/>
        <map:serialize type="xml"/>
      </map:match>
    </map:pipeline>
  </map:pipelines>
</map:sitemap>
